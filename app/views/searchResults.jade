doctype html
html
    head
        title Search Results
        link(rel="stylesheet" href="/client/css/main.css" type="text/css")
    include ./includes/header.jade
    body
        .container-search
            img.logo-header(src="/client/img/StalkMarketLogo.png")
            .search
                form(action="/search" method="get")
                    input.search-bar(type="text" name="stock")
                    input.submit(type="submit" value="SEARCH")
            .results
                h3.results-header Search Results
                each val in results
                    .result
                        a.stock-link(href="/stock/"+val.Symbol)
                            h2.stock-symb= val.Symbol
                        p.stock-name= val.Name
                        p.stock-exch= val.Exchange
        script(type="text/javascript" src="/common/ajax-functions.js")
        script(type="text/javascript" src="/controllers/userController.client.js")